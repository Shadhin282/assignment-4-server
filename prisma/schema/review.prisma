model Review {
    id String @id @default(uuid()) @unique
   studentId String
  tutorId   String
  rating    Int
  comment   String?
  createdAt DateTime @default(now())
  
  student   User     @relation("StudentReviews", fields: [studentId], references: [id])
  tutor     User     @relation("TutorReviews", fields: [tutorId], references: [id])
 
  review    TutorProfile[]
   
    @@index([studentId])
    @@index([tutorId])
}