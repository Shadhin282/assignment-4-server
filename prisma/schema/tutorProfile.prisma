model tutorProfile {
    id String @id @default(uuid())
    name String
    tutorId String
    tutorEmail String
    bio String?
    hourlyRate Float
    subjects String[]
    availability String[]
    categoryId String
    studentRating Int
    reviewId String?
    
    review Review? @relation(fields:[reviewId], references: [id])
    user User @relation(fields: [tutorId], references: [id])
    
    category Category @relation(fields: [categoryId], references: [id])

    @@index([categoryId])
    @@index([tutorId])
}